<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE properties SYSTEM 'http://java.sun.com/dtd/properties.dtd'>

<properties>

    <!-- Transporter Management System topic-->
    <entry key="lanes.topic">lanes</entry>
    <entry key="user.topic">user</entry>
    <entry key="transporter.topic">transporter</entry>
    <entry key="user.email.store">userbyemailstore</entry>
    <entry key="lane.by.uuid.store">lanebyuuidstore</entry>
    <entry key="transporter.state.store">transporterstore</entry>
    <entry key="group.by.id.store">groupbyidstore</entry>

    <!-- Common topics -->
    <entry key="command.topic">command</entry>
    <entry key="command.result.topic">commandresult</entry>

    <!--Stream Configuration -->
    <entry key="application.id">transportmanagement</entry>
    <entry key="transporter.manager.id">transportManager</entry>
    <entry key="bootstrap.server">localhost:9092</entry>
    <entry key="application.server.config">localhost:2223</entry>
    <entry key="auto.offset.reset.config">earliest</entry>
    <entry key="commit.interval.auto.config">500</entry>
    <entry key="schemaregistry"> http://localhost:8081</entry>
    <entry key="base.url">http://localhost:2223</entry>
    <entry key="auto.offset.reset.config">earliest</entry>
    <entry key="commit.interval.auto.config">500</entry>

    <!-- State Store -->
    <entry key="user.state.store">userstatestore</entry>
    <!-- Model Classes -->
    <entry key="model.class">user#com.fretron.Model.User#user.topic transporter#com.fretron.Model.Transporter#transporter.topic lane#com.fretron.Model.Lane#lane.topic
    </entry>
</properties>
